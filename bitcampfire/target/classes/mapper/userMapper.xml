<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="userSQL">
<resultMap type="user" id="userResult"></resultMap>

    <select id="userSignup_idCheck" parameterType="String" resultType="user">
    	select * from user2 where user_id=#{user_id}
    </select>
	
	<insert id="SignupComplete" parameterType="user">
		insert into user2 values(#{user_id}, 
  								#{user_email}, 
  								#{user_pwd}, 
  								#{user_nickname}, 
  								#{user_name}, 
  								#{user_classid}, 
  								#{user_img}, 
  								#{user_auth}, 
  								#{user_grade}, 
  								#{user_ip}, 
  								#{user_salt}, 
  								#{user_social}, 
  								sysdate)
	</insert>

</mapper>